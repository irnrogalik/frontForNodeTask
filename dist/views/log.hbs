{{#if logs}}
<table class="table">
    <thead class="thead-light">
        <tr>
            <th>Date</th>
            <th>Log</th>
        </tr>
    </thead>
    <tbody>
        {{#each logs}}
        <tr>
            <td>{{this.date}}</td>
            <td>{{this.log}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>
<a href="/log/flush">Delete log</a>
{{else}}
{{#if result_flush}}
<p>{{result_flush}}</p>
{{else}}
<p>There is no log</p>
{{/if}}
{{/if}}