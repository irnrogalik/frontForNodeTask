{{#if orderList}}
    <table class="table">
        <thead class="thead-light">
        <tr>
            <th>#</th>
            <th>Date</th>
            <th>Sales taxes</th>
            <th>Total</th>
        </tr>
        </thead>
        <tbody>
        {{#each orderList}}
            <tr>
                <td>{{Id}}</td>
                <td>{{Date}}</td>
                <td>{{SalesTaxes}}</td>
                <td>{{Total}}</td>
                <td>
                    <form action="/cart/delete/{{this.Id}}" method="POST">
                        <input type="submit" value="Delete"/>
                    </form>
                </td>
            </tr>
        {{/each}}
        </tbody>
    </table>
{{else}}
    <p>Order list is empty</p>
{{/if}}
